{
  "buildCommand": "bash build.sh",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/w1/:path*",
      "destination": "/w1/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
